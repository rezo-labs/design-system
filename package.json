{
  "name": "universal-react-app",
  "version": "2.0.0",
  "description": "A boilerplate to create Universal React Application.",
  "repository": {
    "type": "git",
    "url": "https://github.com/duyisking/universal-react-app"
  },
  "scripts": {
    "start": "node configs/scripts/start.js",
    "lint": "eslint src/app -c configs/.eslintrc.js --ext .js,.jsx --ignore-path configs/.eslintignore",
    "test:client": "jest --config ./configs/jest.client.config.js --forceExit",
    "test:server": "jest --config ./configs/jest.server.config.js --forceExit",
    "test": "node configs/scripts/test.js",
    "test:open:client": "node configs/scripts/test-open.js",
    "test:open:server": "node configs/scripts/test-open.js -s",
    "test:open": "npm run test:open:client",
    "test:clean:client": "rimraf ./coverage/client",
    "test:clean:server": "rimraf ./coverage/server",
    "test:clean": "rimraf ./coverage",
    "build": "node configs/scripts/build.js",
    "node": "node configs/scripts/node.js",
    "new-component": "node configs/scripts/new-component",
    "del-component": "node configs/scripts/del-component",
    "storybook": "start-storybook --config-dir configs/.storybook",
    "styleguidist:server": "styleguidist server",
    "styleguidist:build": "styleguidist build",
    "setup": "node configs/scripts/setup/setup.js",
    "internal:test": "jest --config ./configs/jest.internal.config.js --forceExit",
    "internal:test:open": "opener ./configs/coverage/index.html",
    "internal:test:clean": "rimraf ./configs/coverage"
  },
  "license": "MIT",
  "dependencies": {
    "@loadable/component": "^5.9.0",
    "@loadable/server": "^5.9.0",
    "async": "^3.2.2",
    "chalk": "^2.4.2",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "log-symbols": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/types": "^7.4.4",
    "@loadable/babel-plugin": "^5.8.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "@storybook/react": "^5.0.11",
    "@types/jest": "^24.0.16",
    "acorn": "^6.2.1",
    "acorn-jsx": "^5.0.1",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.0",
    "clean-webpack-plugin": "^0.1.19",
    "commander": "^2.20.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^1.1.11",
    "ignore-loader": "^0.1.2",
    "inquirer": "^6.3.1",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.5",
    "opn": "^5.5.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-loader": "^2.1.6",
    "react-styleguidist": "^9.1.13",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "surge": "^0.21.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^3.2.0"
  }
}
